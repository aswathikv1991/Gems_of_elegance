<!DOCTYPE HTML>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>Men Fashion - Admin Dashboard</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="Men Fashion Admin Dashboard">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:title" content="Men Fashion Admin">
    <meta property="og:type" content="website">
    <meta property="og:url" content="">
    <meta property="og:image" content="">
    <link rel="shortcut icon" type="image/x-icon" href="#">
    <link href="/admin-assets/css/main.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@10.16.6/dist/sweetalert2.min.css">
    <link href="/css/adminhead.css" rel="stylesheet" type="text/css" media="all" />

    <script src="https://kit.fontawesome.com/16d4885483.js" crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>

  
</head>
<body>

    <header class="main-header">
        <div class="col-nav">
            <button class="btn btn-icon btn-mobile me-auto" data-trigger="#offcanvas_aside">
                <i class="material-icons md-apps"></i>
            </button>
            <div class="logo-title">
                <img class="logo-img" src="/images/gems.png" alt="Logo">
                <h2>Gems of Elegance</h2>
            </div>
            <ul class="nav">
                <li class="dropdown nav-item">
                    <a class="dropdown-toggle" data-bs-toggle="dropdown" href="#">
                        
                    </a>
                    <div class="dropdown-menu dropdown-menu-end">
                        <div class="dropdown-divider"></div>
                        <button><a class="dropdown-item text-danger" href="/admin/logout">
                            <i class="material-icons md-exit_to_app"></i> Logout
                        </a></button>
                    </div>
                </li>
            </ul>
        </div>
    </header>
    
    
    <!-- Sidebar -->
    <aside class="navbar-aside" id="offcanvas_aside">
        <nav>
            <ul class="menu-aside">
                <li class="menu-item">
                    <a class="menu-link" href="/admin">
                        <i class="icon material-icons md-home"></i> Dashboard
                    </a>
                </li>
                <li class="menu-item">
                    <a class="menu-link" href="/admin/users">
                        <i class="icon material-icons md-person"></i> Customers
                    </a>
                </li>
                <li class="menu-item">
                    <a class="menu-link" href="/admin/category">
                        <i class="icon material-icons md-category"></i> Category
                    </a>
                </li>
                <li class="menu-item">
                    <a class="menu-link" href="/admin/brands">
                        <i class="icon material-icons md-stars"></i> Brands
                    </a>
                </li>
                <li class="menu-item">
                    <a class="menu-link" href="/admin/products">
                        <i class="icon material-icons md-shopping_bag"></i> Products
                    </a>
                </li>
                <li class="menu-item">
                    <a class="menu-link" href="/admin/orderList">
                        <i class="icon material-icons md-shopping_cart"></i> Orders
                    </a>
                </li>
            </ul>
        </nav>
    </aside>

    <!-- Main Content -->
   
    <!-- JavaScript -->
    <script>
        $(document).ready(function() {
            $('.btn-mobile').on('click', function() {
                $('#offcanvas_aside').toggleClass('active');
            });
        });
    </script>
</body>
</html>
